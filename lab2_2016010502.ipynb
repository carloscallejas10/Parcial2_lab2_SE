    "import time\n",
    "import numpy as np\n",
    "suma =0\n",
    "inicio = time.time()\n",
    "random =np.random.normal(500, 30, size=(10000000))\n",
    "for menor in random:\n",
    "    if int(menor) <=500000:\n",
    "        suma= int(suma) + int(menor)\n",
    "\n"
